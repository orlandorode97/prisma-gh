generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DSN")
}

model my_new_table {
  id         Int      @id @default(autoincrement()) @db.UnsignedInt
  created_at DateTime @default(now()) @db.Timestamp(0)
}

model model {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
}

model rename {
  id        Int     @id @default(autoincrement())
  is_active Boolean
}

model user {
  id    Int    @id @default(autoincrement())
  email String @unique
}
